{
  "name": "memory-usage-chart",
  "version": "18.12.4",
  "description": "Simple NodeJS app that displays CPU free memory",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index",
    "build": "tsc && npm run build-ui",
    "build-ui": "webpack ./dist/public/app.js --config webpack-config.js && npm run html",
    "html": "copyfiles public/**/*.html public/**/*.css dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yohannes Seifu",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "connect": "^3.6.6",
    "copyfiles": "^2.1.0",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "request": "^2.88.0",
    "ts-loader": "^5.3.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.3.3",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "typescript": "^3.2.1"
  }
}
